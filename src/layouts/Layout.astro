---
import '~/styles/tailwind.css'
import SentryClient from '@altipla/sentry-astro/SentryClient.astro'

let generator = Astro.generator
---

<!doctype html>
<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <link rel='icon' type='image/svg+xml' href='/favicon.svg' />
    <meta name='generator' content={generator} />
    <SentryClient />
    <link
      href='https://fonts.googleapis.com/css2?family=Inter:wght@100..900&family=Poppins:wght@400;500&family=Roboto+Mono:wght@400;500;600;700&display=swap'
      rel='stylesheet'
    />

    <title>Project Name</title>
  </head>
  <body>
    <slot />
  </body>
</html>
